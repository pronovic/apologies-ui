{
  "name": "apologies-ui",
  "version": "0.1.0",
  "description": "Front-end to play a game similar to Sorry",
  "main": "index.js",
  "keywords": [],
  "author": "Kenneth J. Pronovici <pronovic@ieee.org>",
  "license": "Apache-2.0",
  "scripts": {
    "checks": "eslint --max-warnings=0 '**/*.js' '**/*.vue'",
    "format": "eslint --fix --max-warnings=0 '**/*.js' '**/*.vue'",
    "clean": "rm -rf 'dist/*'",
    "rmcache": "rm -rf '.cache'",
    "build": "yarn clean && parcel build src/index.html",
    "server": "parcel serve src/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "alias": {
    "Assets": "./src/assets",
    "Components": "./src/components",
    "Router": "./src/router",
    "VStore": "./src/store",
    "Utils": "./src/utils",
    "Views": "./src/views"
  },
  "jest": {
    "moduleNameMapper": {
      "^Assets(.*)$": "<rootDir>/src/assets$1",
      "^Components(.*)$": "<rootDir>/src/components$1",
      "^Router(.*)$": "<rootDir>/src/router$1",
      "^VStore(.*)$": "<rootDir>/src/store$1",
      "^Utils(.*)$": "<rootDir>/src/utils$1",
      "^Views(.*)$": "<rootDir>/src/views$1"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@vue/component-compiler-utils": "^3.1.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cypress": "^4.6.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^7.0.0-alpha.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "atmosphere.js": "^3.0.4",
    "bootstrap-vue": "^2.15.0",
    "components": "^0.1.0",
    "core-js": "^3.6.5",
    "gsap": "^3.3.1",
    "konva": "^6.0.0",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4",
    "vue-konva": "^2.1.3",
    "vue-router": "^3.2.0",
    "vuetify": "^2.2.29",
    "vuex": "^3.4.0"
  }
}
